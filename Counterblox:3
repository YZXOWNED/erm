local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local WhitelistedHWIDs = {"21B24C27-258E-40E0-B998-DEF0252D7D76", "541F1F61-AB6C-408E-9D85-C98736EADBE2","E5CB0A22-807B-4108-9D75-57267C4F5F69","",""} -- Add your whitelisted HWIDs here

function CheckHWID(hwidval)
    for _, whitelisted in pairs(WhitelistedHWIDs) do
        if hwidval == whitelisted then
            return true
        end
    end
    return false
end

local isWhitelisted = CheckHWID(HWID)

-- Map each game's PlaceId to the corresponding library URL
local gameLibraries = {
    [301549746] = "https://raw.githubusercontent.com/YZXOWNED/newscript/refs/heads/main/whatulookingforskidder%3F",--counter
    [4483381587] = "https://raw.githubusercontent.com/YZXOWNED/newscript/refs/heads/main/counterbloxtf%24",--baseplate for testing
    [286090429] = "https://raw.githubusercontent.com/YZXOWNED/bestarsenalscriptfuckthaskidders/refs/heads/main/imcooked",--arsenal
    [1869597719] = "https://raw.githubusercontent.com/YZXOWNED/newscript/refs/heads/main/whatulookingforskidder%3F",--counter blox deathmatch

}
local function loadLibraryForGame()
    local currentPlaceId = game.PlaceId
    local libraryUrl = gameLibraries[currentPlaceId]
    
    if libraryUrl then
        local success, library = pcall(function()
            return loadstring(game:HttpGet(libraryUrl))()
        end)

        if success then
            print("Hwid In Whitelisted Script Loaded")
        else
            warn("Failed to load the Script (No script or Loaded Right) " .. currentPlaceId)
        end
    else
        warn("Failed to load the Script (No script or not added game ID)")
    end
end

if isWhitelisted then
    loadLibraryForGame()
print(" Script Loaded")
else
    warn("Your HWID is not whitelisted. Join the Discord and make a ticket.")
end

