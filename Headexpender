-- Define Whitelisted HWIDs
local WhitelistedHWIDs = {
    ["41D9CEC1-2927-43F1-94FA-E90F7040AF6F"] = true,
    ["E7C4E451-9524-4FE0-A263-78C2A04BD4BA"] = true,
    
}

-- Get current HWID
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()

-- Main Execution Block
if WhitelistedHWIDs[HWID] then
    local success, err = pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/YZXOWNED/library.new/refs/heads/main/Paidcooked"))()
    end)
    if success then
        print("Script loaded successfully")
    else
        warn("Error loading script: " .. err)
    end
else
    game.Players.LocalPlayer:Kick("Your HWID is not whitelisted. Access denied.")
end
